{
  "name": "@shopgate/tracking-ga-native",
  "version": "1.3.1",
  "description": "Shopgate's Google Analytics native tracker",
  "main": "frontend/index.js",
  "repository": "shopgate/tracking-ga-native",
  "bugs": "https://github.com/shopgate/tracking-ga-native/issues",
  "homepage": "https://github.com/shopgate/tracking-ga-native",
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint --parser babel-eslint --ignore-path .gitignore  --ext .js .",
    "test": "jest",
    "coverage": "jest --coverage",
    "prebuild": "rimraf ./dist",
    "build": "NODE_ENV=production babel ./ --out-dir dist --no-comments --minified --ignore tests,spec.js,spec.jsx,__snapshots__,.eslintrc.js,jest.config.js,dist,coverage,node_modules",
    "postbuild": "cp {package.json,package-lock.json,../CHANGELOG.md,../LICENSE.md,../README.md} ./dist/ && git add -A",
    "version": "npm run build && PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && bump ../extension-config.json -v $PACKAGE_VERSION -y",
    "postversion": "PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && cd ../ && git add -A && git commit -m \"$PACKAGE_VERSION\" && git push && git push --tags && cd ./frontend/dist/ && npm publish --access public"
  },
  "dependencies": {
    "@shopgate/tracking-core": "^0.1.3",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-runtime": "^6.26.0"
  },
  "devDependencies": {
    "@shopgate/eslint-config": "^1.0.2",
    "@shopgate/pwa-common": "^0.1.11",
    "@shopgate/pwa-core": "^0.1.5",
    "babel-cli": "^6.26.0",
    "jest": "^21.2.1",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "rimraf": "^2.6.2"
  },
  "peerDependencies": {
    "@shopgate/pwa-common": "0.1.x"
  }
}
